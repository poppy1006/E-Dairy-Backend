{
  "name": "swathantra-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:local": "env-cmd -f .env.local ts-node-dev --watch --respawn --transpile-only --exit-child src/main.ts",
    "start:dev": "env-cmd -f .env.development ts-node-dev --watch --respawn --transpile-only --exit-child src/main.ts",
    "start:prod": "env-cmd -f .env.production ts-node-dev --watch --respawn --transpile-only --exit-child src/main.ts",
    "build": "npx tsc",
    "lint": "eslint . --ext .ts",
    "start:server:local": "env-cmd -f .env.local node dist/main.js",
    "start:server:dev": "env-cmd -f .env.development node dist/main.js",
    "start:server:prod": "env-cmd -f .env.production node dist/main.js",
    "db:generate:client": "npx prisma generate",
    "db:migrate:local": "env-cmd -f .env.local -- npx prisma migrate dev",
    "db:migrate:dev": "env-cmd -f .env.development -- npx prisma migrate dev",
    "db:seed:local": "env-cmd -f .env.local -- npx ts-node --transpile-only src/prisma/seeder/index.ts",
    "db:seed:dev": "env-cmd -f .env.development -- npx ts-node --transpile-only src/prisma/seeder/index.ts",
    "db:reset:local": "env-cmd -f .env.local -- npx prisma migrate reset",
    "db:reset:dev": "env-cmd -f .env.development -- npx prisma migrate reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@types/string-template": "^1.0.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "aws-sdk": "^2.1652.0",
    "eslint": "^9.6.0",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.5.3"
  },
  "dependencies": {
    "@faatlab/express-decorator": "^0.2.7",
    "@faatlab/express-response-formatter": "^0.3.4",
    "@godaddy/terminus": "^4.12.1",
    "@prisma/client": "^5.16.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.2",
    "nodemailer": "^6.9.14",
    "redis": "^4.6.14",
    "string-template": "^1.0.0",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "winston-cloudwatch": "^6.3.0",
    "zod": "^3.23.8",
    "zod-error": "^1.5.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
